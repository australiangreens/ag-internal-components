    "jotai": "^2.14.0",
    "react-avatar": "^5.0.4",
    "react-transition-group": "^4.4.5",
    "souvlaki": "^0.3.1"
  },
  "peerDependencies": {
    "@auth0/auth0-react": "^2.5.0",
